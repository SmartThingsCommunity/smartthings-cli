{
	"name": "@smartthings/cli",
	"version": "1.10.5",
	"description": "Command Line Interface for the SmartThings APIs",
	"keywords": [
		"smartthings",
		"cli"
	],
	"homepage": "https://github.com/SmartThingsCommunity/smartthings-cli",
	"author": "Samsung Electronics Co., LTD.",
	"bugs": {
		"url": "https://github.com/SmartThingsCommunity/smartthings-cli/issues"
	},
	"license": "Apache-2.0",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/SmartThingsCommunity/smartthings-cli.git"
	},
	"files": [
		"bin",
		"dist/src",
		"dist/tsconfig.tsbuildinfo",
		"README.md",
		"/npm-shrinkwrap.json",
		"!*/src/__tests__"
	],
	"main": "./dist/src/index.js",
	"bin": {
		"smartthings": "dist/src/run.js"
	},
	"exports": {
		".": "./dist/src/index.js"
	},
	"engines": {
		"node": ">=22.9.0 <23",
		"npm": ">=10.8.3"
	},
	"publishConfig": {
		"access": "public"
	},
	"type": "module",
	"pkg": {
		"scripts": [
			"dist/**/*.js"
		],
		"assets": [
			"../../node_modules/yarn"
		],
		"outputPath": "dist_bin",
		"targets": [
			"node18-macos-x64",
			"node18-macos-arm64",
			"node18-linux-x64",
			"node18-linux-arm64",
			"node18-linuxstatic-armv7",
			"node18-win-x64"
		]
	},
	"dependencies": {
		"@aws-sdk/client-lambda": "^3.817.0",
		"@inquirer/prompts": "^7.5.3",
		"@smartthings/core-sdk": "^8.4.1",
		"axios": "1.10.0",
		"chalk": "^5.4.1",
		"env-paths": "^3.0.0",
		"eventsource": "^4.0.0",
		"express": "^5.1.0",
		"get-port-please": "^3.1.2",
		"inquirer": "^9.3.7",
		"js-yaml": "^4.1.0",
		"jszip": "^3.10.1",
		"lodash.at": "^4.6.0",
		"log4js": "^6.9.1",
		"open": "^10.1.2",
		"ora": "^8.2.0",
		"os-locale": "^6.0.2",
		"picomatch": "^4.0.2",
		"qs": "^6.14.0",
		"table": "^6.9.0",
		"tslib": "^2.8.1",
		"undici": "^7.11.0",
		"uuid": "^11.1.0",
		"yargs": "^18.0.0"
	},
	"devDependencies": {
		"@changesets/changelog-github": "^0.5.1",
		"@changesets/cli": "^2.29.4",
		"@changesets/release-utils": "^0.2.5",
		"@commitlint/cli": "^19.8.1",
		"@commitlint/config-conventional": "^19.8.1",
		"@stylistic/eslint-plugin": "^2.8.0",
		"@types/express": "^5.0.2",
		"@types/inquirer": "^9.0.7",
		"@types/jest": "^29.5.14",
		"@types/js-yaml": "^4.0.9",
		"@types/lodash.at": "^4.6.9",
		"@types/node": "^22.15.24",
		"@types/picomatch": "^4.0.0",
		"@types/qs": "^6.14.0",
		"@types/uuid": "^10.0.0",
		"@types/yargs": "^17.0.33",
		"@typescript-eslint/eslint-plugin": "^8.6.0",
		"@typescript-eslint/parser": "^8.6.0",
		"archiver": "^7.0.1",
		"cz-conventional-changelog": "^3.3.0",
		"eslint": "^8.57.1",
		"eslint-plugin-eslint-comments": "^3.2.0",
		"eslint-plugin-import": "^2.30.0",
		"eslint-plugin-jest": "^28.8.3",
		"jest": "^29.7.0",
		"jest-environment-jsdom": "^29.7.0",
		"jest-extended": "^5.0.3",
		"jest-html-reporters": "^3.1.7",
		"mock-stdin": "^1.0.0",
		"pkg": "^5.8.1",
		"rimraf": "^6.0.1",
		"strip-ansi": "^7.1.0",
		"ts-jest": "^29.3.4",
		"ts-node": "^10.9.2",
		"typescript": "^5.8.3"
	},
	"scripts": {
		"lint": "eslint --ext ts src",
		"format": "eslint --ext ts src --fix",
		"clean": "rimraf dist && rimraf tsconfig.tsbuildinfo",
		"full-clean": "rimraf dist_bin && rimraf coverage && rimraf jest*html* && npm run clean && rimraf node_modules",
		"compile": "tsc",
		"readme": "echo TODO:readme",
		"watch": "tsc -w --sourceMap",
		"build": "npm run clean && npm run compile && npm run readme",
		"debug-build": "npm run clean && tsc --sourceMap && npm run readme",
		"test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
		"test-watch": "node --experimental-vm-modules node_modules/jest/bin/jest.js --watch --reporters=default",
		"test-coverage": "node --experimental-vm-modules node_modules/jest/bin/jest.js --coverage=true",
		"prepack": "tsc && echo TODO:readme",
		"version": "echo TODO:readme && git add README.md && changeset version && npm i --package-lock-only && npm run build && npm run version -w packages/cli",
		"release": "npm run build && changeset publish",
		"package": "pkg package.json && npm run zip-binaries",
		"zip-binaries": "node zip-binaries.mjs"
	},
	"directories": {
		"doc": "doc"
	}
}
