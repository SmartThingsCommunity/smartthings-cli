{
	"name": "@smartthings/cli",
	"description": "SmartThings unified CLI",
	"version": "0.0.0",
	"author": "SmartThings, Inc.",
	"bin": {
		"smartthings": "./bin/run"
	},
	"bugs": "https://github.com/SmartThingsCommunity/smartthings-cli/issues",
	"dependencies": {
		"@oclif/command": "^1.5.19",
		"@oclif/config": "^1.13.3",
		"@oclif/plugin-autocomplete": "^0.1.5",
		"@oclif/plugin-help": "^2.2.3",
		"@oclif/plugin-not-found": "^1.2.3",
		"generator-smartthings": "^1.5.0",
		"js-yaml": "^3.13.1",
		"tslib": "^1.10.0",
		"yeoman-environment": "^2.7.0"
	},
	"devDependencies": {
		"@oclif/dev-cli": "^1.22.2",
		"@oclif/test": "^1.2.5",
		"@types/chai": "^4.2.5",
		"@types/js-yaml": "^3.12.1",
		"@types/mocha": "^5.2.7",
		"@types/node": "^10.17.11",
		"@types/qs": "^6.9.0",
		"@types/yeoman-environment": "^2.3.2",
		"@typescript-eslint/eslint-plugin": "^2.12.0",
		"@typescript-eslint/parser": "^2.12.0",
		"chai": "^4.2.0",
		"eslint": "^6.7.2",
		"globby": "^10.0.1",
		"mocha": "^6.2.2",
		"nyc": "^14.1.1",
		"ts-node": "^8.5.4",
		"typescript": "^3.7.3"
	},
	"engines": {
		"node": ">=10.0.0"
	},
	"files": [
		"/bin",
		"/lib",
		"/npm-shrinkwrap.json",
		"/oclif.manifest.json"
	],
	"homepage": "https://github.com/SmartThingsCommunity/smartthings-cli",
	"keywords": [
		"oclif"
	],
	"license": "Apache-2.0",
	"main": "lib/index.js",
	"oclif": {
		"commands": "./lib/commands",
		"bin": "smartthings",
		"plugins": [
			"@oclif/plugin-help",
			"@oclif/plugin-autocomplete",
			"@oclif/plugin-not-found"
		],
		"hooks": {
			"init": "./lib/hooks/init/init-config"
		}
	},
	"repository": "SmartThingsCommunity/smartthings-cli",
	"scripts": {
		"lint": "eslint src/**/*.ts",
		"postpack": "rm -f oclif.manifest.json",
		"prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
		"test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
		"version": "oclif-dev readme && git add README.md"
	},
	"types": "lib/index.d.ts"
}
