{
	"name": "@smartthings/cli",
	"version": "1.8.0",
	"description": "Command Line Interface for the SmartThings APIs",
	"keywords": ["smartthings", "cli"],
	"homepage": "https://github.com/SmartThingsCommunity/smartthings-cli",
	"author": "Samsung Electronics Co., LTD.",
	"bugs": {
		"url": "https://github.com/SmartThingsCommunity/smartthings-cli/issues"
	},
	"license": "Apache-2.0",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/SmartThingsCommunity/smartthings-cli.git"
	},
	"files": [
		"bin",
		"dist/src",
		"dist/tsconfig.tsbuildinfo",
		"README.md",
		"/npm-shrinkwrap.json",
		"!*/src/__tests__"
	],
	"main": "./dist/src/index.js",
	"bin": {
		"smartthings": "dist/src/run.js"
	},
	"exports": {
		".": "./dist/src/index.js"
	},
	"engines": {
		"node": ">=20.8.0 <21",
		"npm": ">=10.1"
	},
	"publishConfig": {
		"access": "public"
	},
	"type": "module",
	"pkg": {
		"scripts": [
			"dist/**/*.js"
		],
		"assets": [
			"../../node_modules/yarn"
		],
		"outputPath": "dist_bin",
		"targets": [
			"node18-macos-x64",
			"node18-macos-arm64",
			"node18-linux-x64",
			"node18-linux-arm64",
			"node18-linuxstatic-armv7",
			"node18-win-x64"
		]
	},
	"dependencies": {
		"@aws-sdk/client-lambda": "^3.427.0",
		"@smartthings/core-sdk": "8.0.1",
		"axios": "1.5.0",
		"chalk": "^5.3.0",
		"eventsource": "^2.0.2",
		"express": "^4.18.2",
		"get-port-please": "^3.1.1",
		"inquirer": "^9.2.11",
		"js-yaml": "^4.1.0",
		"jszip": "^3.10.1",
		"lodash.at": "^4.6.0",
		"log4js": "^6.9.1",
		"open": "^9.1.0",
		"ora": "^7.0.1",
		"os-locale": "^6.0.2",
		"picomatch": "^2.3.1",
		"qs": "^6.11.2",
		"table": "^6.8.1",
		"tslib": "^2.6.2",
		"uuid": "^9.0.1",
		"yargs": "^17.7.2"
	},
	"devDependencies": {
		"@changesets/changelog-github": "^0.4.8",
		"@changesets/cli": "^2.26.2",
		"@changesets/release-utils": "^0.1.13",
		"@commitlint/cli": "^17.7.2",
		"@commitlint/config-conventional": "^17.7.0",
		"@types/eventsource": "^1.1.12",
		"@types/express": "^4.17.18",
		"@types/inquirer": "^9.0.6",
		"@types/jest": "^29.5.5",
		"@types/js-yaml": "^4.0.6",
		"@types/lodash.at": "^4.6.7",
		"@types/node": "^20.8.4",
		"@types/picomatch": "^2.3.1",
		"@types/qs": "^6.9.8",
		"@types/uuid": "^9.0.5",
		"@types/yargs": "^17.0.29",
		"@typescript-eslint/eslint-plugin": "^6.7.5",
		"@typescript-eslint/parser": "^6.7.5",
		"archiver": "^6.0.1",
		"cz-conventional-changelog": "^3.3.0",
		"eslint": "^8.51.0",
		"eslint-plugin-eslint-comments": "^3.2.0",
		"eslint-plugin-import": "^2.28.1",
		"eslint-plugin-jest": "^27.4.2",
		"jest": "^29.7.0",
		"jest-environment-jsdom": "^29.7.0",
		"jest-extended": "^4.0.2",
		"jest-html-reporters": "^3.1.4",
		"mock-stdin": "^1.0.0",
		"pkg": "^5.8.1",
		"rimraf": "^5.0.5",
		"ts-jest": "^29.1.1",
		"ts-node": "^10.9.1",
		"typescript": "^5.2.2"
	},
	"scripts": {
		"lint": "eslint --ext ts src",
		"format": "eslint --ext ts src --fix",
		"clean": "rimraf dist && rimraf tsconfig.tsbuildinfo",
		"full-clean": "rimraf dist_bin && rimraf coverage && rimraf jest*html* && npm run clean && rimraf node_modules",
		"compile": "tsc",
		"readme": "echo TODO:readme",
		"watch": "tsc -w --sourceMap",
		"build": "npm run clean && npm run compile && npm run readme",
		"debug-build": "npm run clean && tsc --sourceMap && npm run readme",
		"test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
		"test-watch": "node --experimental-vm-modules node_modules/jest/bin/jest.js --watch --reporters=default",
		"test-coverage": "node --experimental-vm-modules node_modules/jest/bin/jest.js --coverage=true",
		"prepack": "tsc && echo TODO:readme",
		"version": "echo TODO:readme && git add README.md && changeset version && npm i --package-lock-only && npm run build && npm run version -w packages/cli",
		"release": "npm run build && changeset publish",
		"package": "pkg package.json && npm run zip-binaries",
		"zip-binaries": "node zip-binaries.mjs"
	},
	"directories": {
		"doc": "doc"
	}
}
