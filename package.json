{
	"name": "@smartthings/cli",
	"version": "1.3.1",
	"description": "Command Line Interface for the SmartThings APIs",
	"author": "SmartThings, Inc.",
	"bin": {
		"smartthings": "bin/run"
	},
	"bugs": {
		"url": "https://github.com/SmartThingsCommunity/smartthings-cli/issues"
	},
	"homepage": "https://github.com/SmartThingsCommunity/smartthings-cli",
	"license": "Apache-2.0",
	"main": "dist/index.js",
	"engines": {
		"node": ">=18.15.0 <19",
		"npm": ">=9.5"
	},
	"files": [
		"bin",
		"dist",
		"README.md",
		"/npm-shrinkwrap.json",
		"!*/__tests__"
	],
	"publishConfig": {
		"access": "public"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/SmartThingsCommunity/smartthings-cli.git"
	},
	"type": "module",
	"pkg": {
		"scripts": [
			"dist/**/*.js"
		],
		"assets": [
			"../../node_modules/yarn"
		],
		"outputPath": "dist_bin",
		"targets": [
			"node18-macos-x64",
			"node18-macos-arm64",
			"node18-linux-x64",
			"node18-linux-arm64",
			"node18-linuxstatic-armv7",
			"node18-win-x64"
		]
	},
	"dependencies": {
		"@aws-sdk/client-lambda": "^3.329.0",
		"@log4js-node/log4js-api": "^1.0.2",
		"@smartthings/core-sdk": "^7.1.0",
		"axios": "^0.27.2",
		"chalk": "^4.1.2",
		"eventsource": "^2.0.2",
		"express": "^4.18.1",
		"get-port": "^5.1.1",
		"inquirer": "^8.2.4",
		"js-yaml": "^4.1.0",
		"jszip": "^3.9.1",
		"lodash.at": "^4.6.0",
		"log4js": "^6.6.0",
		"open": "^8.4.0",
		"os-locale": "^5.0.0",
		"picomatch": "^2.3.1",
		"qs": "^6.11.1",
		"table": "^6.8.0",
		"tslib": "^2.3.1",
		"uuid": "^9.0.0"
	},
	"devDependencies": {
		"@changesets/changelog-github": "^0.4.6",
		"@changesets/cli": "^2.24.4",
		"@changesets/release-utils": "^0.1.10",
		"@commitlint/cli": "^17.1.2",
		"@commitlint/config-conventional": "^17.1.0",
		"@types/eventsource": "^1.1.9",
		"@types/express": "^4.17.13",
		"@types/inquirer": "^8.2.1",
		"@types/jest": "^29.5.1",
		"@types/js-yaml": "^4.0.5",
		"@types/lodash.at": "^4.6.7",
		"@types/node": "^18.15.11",
		"@types/picomatch": "^2.3.0",
		"@types/qs": "^6.9.7",
		"@types/uuid": "^9.0.1",
		"@typescript-eslint/eslint-plugin": "^5.59.7",
		"@typescript-eslint/parser": "^5.59.7",
		"archiver": "^5.3.1",
		"cz-conventional-changelog": "^3.3.0",
		"eslint": "^8.41.0",
		"eslint-plugin-eslint-comments": "^3.2.0",
		"eslint-plugin-import": "^2.27.5",
		"eslint-plugin-jest": "^27.2.1",
		"jest": "^29.5.0",
		"jest-environment-jsdom": "^29.5.0",
		"jest-extended": "^3.2.4",
		"jest-html-reporters": "^3.1.4",
		"mock-stdin": "^1.0.0",
		"pkg": "^5.8.0",
		"rimraf": "^3.0.2",
		"ts-jest": "^29.1.0",
		"ts-node": "^10.9.1",
		"typescript": "^4.8.3"
	},
	"scripts": {
		"lint": "eslint --ext ts src",
		"format": "eslint --ext ts src --fix",
		"clean": "rimraf dist && rimraf tsconfig.tsbuildinfo",
		"full-clean": "rimraf dist_bin && rimraf coverage && rimraf jest*html* && npm run clean && rimraf node_modules",
		"compile": "tsc -b",
		"readme": "echo TODO:readme",
		"watch": "tsc -b -w",
		"build": "npm run clean && npm run compile && npm run readme",
		"test": "jest",
		"test-watch": "jest --watch --reporters=default",
		"test-coverage": "jest --coverage=true",
		"prepack": "tsc -b && echo TODO:readme",
		"version": "echo TODO:readme && git add README.md && changeset version && npm i --package-lock-only && npm run build && npm run version -w packages/cli",
		"release": "npm run build && changeset publish",
		"package": "pkg package.json && npm run zip-binaries",
		"zip-binaries": "node zip-binaries.mjs"
	},
	"directories": {
		"doc": "doc"
	}
}
