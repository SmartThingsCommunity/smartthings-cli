{
	"name": "@smartthings/cli",
	"version": "1.1.0",
	"description": "Command Line Interface for the SmartThings APIs",
	"author": "SmartThings, Inc.",
	"bin": {
		"smartthings": "bin/run"
	},
	"bugs": {
		"url": "https://github.com/SmartThingsCommunity/smartthings-cli/issues"
	},
	"homepage": "https://github.com/SmartThingsCommunity/smartthings-cli",
	"license": "Apache-2.0",
	"main": "lib/index.js",
	"engines": {
		"node": ">=16.14.2 <17",
		"npm": ">=8"
	},
	"files": [
		"/bin",
		"/lib",
		"README.md",
		"/npm-shrinkwrap.json",
		"/oclif.manifest.json",
		"!*/__tests__"
	],
	"publishConfig": {
		"access": "public"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/SmartThingsCommunity/smartthings-cli.git"
	},
	"keywords": [
		"oclif"
	],
	"oclif": {
		"commands": "./lib/commands",
		"bin": "smartthings",
		"plugins": [
			"@smartthings/plugin-cli-edge",
			"@oclif/plugin-autocomplete",
			"@oclif/plugin-help",
			"@oclif/plugin-not-found",
			"@oclif/plugin-plugins"
		],
		"hooks": {
			"init": "./lib/hooks/init/init-config"
		},
		"repositoryPrefix": "<%- repo %>/blob/@smartthings/cli@<%- version %>/packages/cli/<%- commandPath %>",
		"additionalHelpFlags": [
			"-h"
		]
	},
	"pkg": {
		"scripts": [
			"lib/**/*.js"
		],
		"assets": [
			"../../node_modules/yarn"
		],
		"outputPath": "dist_bin",
		"targets": [
			"node16-macos-x64",
			"node16-macos-arm64",
			"node16-linux-x64",
			"node16-linux-arm64",
			"node16-linuxstatic-armv7",
			"node16-win-x64"
		]
	},
	"dependencies": {
		"@oclif/core": "^1.16.3",
		"@oclif/plugin-autocomplete": "^1.3.0",
		"@oclif/plugin-help": "^5.1.12",
		"@oclif/plugin-not-found": "^2.3.1",
		"@oclif/plugin-plugins": "^2.1.0",
		"@smartthings/cli-lib": "^1.1.0",
		"@smartthings/core-sdk": "^5.2.0",
		"@smartthings/plugin-cli-edge": "^2.0.1",
		"aws-sdk": "^2.1175.0",
		"inquirer": "^8.2.4",
		"js-yaml": "^4.1.0",
		"log4js": "^6.6.0"
	},
	"devDependencies": {
		"@smartthings/cli-testlib": "^1.1.0",
		"@types/inquirer": "^8.2.1",
		"@types/jest": "^28.1.5",
		"@types/js-yaml": "^4.0.5",
		"@types/node": "^16.11.44",
		"@typescript-eslint/eslint-plugin": "^5.30.6",
		"@typescript-eslint/parser": "^5.30.6",
		"archiver": "^5.3.1",
		"eslint": "^8.19.0",
		"eslint-plugin-eslint-comments": "^3.2.0",
		"eslint-plugin-import": "^2.26.0",
		"eslint-plugin-jest": "^26.6.0",
		"jest": "^28.1.3",
		"jest-extended": "^0.11.5",
		"jest-html-reporters": "^3.0.11",
		"oclif": "^3.1.2",
		"pkg": "^5.8.0",
		"rimraf": "^3.0.2",
		"ts-jest": "^28.0.6",
		"ts-node": "^10.9.1",
		"typescript": "^4.8.3"
	},
	"scripts": {
		"lint": "eslint --ext ts src",
		"format": "eslint --ext ts src --fix",
		"clean": "rimraf lib && rimraf tsconfig.tsbuildinfo",
		"full-clean": "rimraf node_modules && rimraf dist_bin && rimraf coverage && rimraf jest*html*reporters* && npm run clean",
		"compile": "tsc -b",
		"readme": "oclif readme",
		"watch": "tsc -b -w",
		"build": "npm run clean && npm run compile",
		"test": "jest",
		"test-watch": "jest --watch --reporters=default",
		"test-coverage": "jest --coverage=true",
		"postpack": "rimraf oclif.manifest.json",
		"prepack": "tsc -b && oclif manifest && oclif readme",
		"version": "oclif readme && git add README.md",
		"package": "pkg package.json && npm run zip-binaries",
		"zip-binaries": "node zip-binaries.mjs"
	},
	"directories": {
		"doc": "doc"
	}
}
