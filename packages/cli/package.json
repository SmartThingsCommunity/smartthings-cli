{
	"name": "@smartthings/cli",
	"version": "0.0.0-pre.32",
	"description": "SmartThings unified CLI",
	"author": "SmartThings, Inc.",
	"bin": {
		"smartthings": "./bin/run"
	},
	"bugs": "https://github.com/SmartThingsCommunity/smartthings-cli/issues",
	"homepage": "https://github.com/SmartThingsCommunity/smartthings-cli",
	"license": "Apache-2.0",
	"main": "lib/index.js",
	"engines": {
		"node": ">=12.13.1"
	},
	"files": [
		"/bin",
		"/lib",
		"README.md",
		"/npm-shrinkwrap.json",
		"/oclif.manifest.json",
		"!*/__tests__"
	],
	"publishConfig": {
		"access": "public"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/SmartThingsCommunity/smartthings-cli.git"
	},
	"keywords": [
		"oclif"
	],
	"oclif": {
		"commands": "./lib/commands",
		"bin": "smartthings",
		"plugins": [
			"@smartthings/plugin-cli-edge",
			"@oclif/plugin-help",
			"@oclif/plugin-autocomplete",
			"@oclif/plugin-not-found",
			"@oclif/plugin-plugins"
		],
		"hooks": {
			"init": "./lib/hooks/init/init-config"
		},
		"repositoryPrefix": "<%- repo %>/blob/v<%- version %>/packages/cli/<%- commandPath %>"
	},
	"pkg": {
		"scripts": [
			"lib/**/*.js"
		],
		"assets": [
			"../../node_modules/generator-smartthings",
			"../../node_modules/istextorbinary",
			"../../node_modules/yarn"
		]
	},
	"dependencies": {
		"@oclif/command": "1.8.0",
		"@oclif/config": "1.17.0",
		"@oclif/errors": "1.3.5",
		"@oclif/plugin-autocomplete": "^0.3.0",
		"@oclif/plugin-help": "^3.2.3",
		"@oclif/plugin-not-found": "^1.2.4",
		"@oclif/plugin-plugins": "^1.10.1",
		"@smartthings/cli-lib": "^0.0.0-pre.32",
		"@smartthings/core-sdk": "^1.11.0",
		"@smartthings/plugin-cli-edge": "^1.5.0",
		"aws-sdk": "^2.690.0",
		"generator-smartthings": "^1.5.0",
		"inquirer": "^8.1.2",
		"lodash": "^4.17.19",
		"open": "^7.0.3",
		"yeoman-environment": "^2.9.1"
	},
	"devDependencies": {
		"@oclif/dev-cli": "^1.26.0",
		"@smartthings/cli-testlib": "^0.0.0-pre.32",
		"@types/cli-table": "^0.3.0",
		"@types/inquirer": "^7.3.3",
		"@types/jest": "^26.0.14",
		"@types/js-yaml": "^3.12.3",
		"@types/lodash": "^4.14.157",
		"@types/node": "^12.12.67",
		"@types/uuid": "^8.3.0",
		"@types/yeoman-environment": "^2.3.3",
		"@typescript-eslint/eslint-plugin": "^4.4.0",
		"@typescript-eslint/parser": "^4.4.0",
		"eslint": "^7.14.0",
		"eslint-plugin-eslint-comments": "^3.2.0",
		"eslint-plugin-import": "^2.22.1",
		"eslint-plugin-jest": "^24.1.3",
		"jest": "^26.5.2",
		"jest-extended": "^0.11.5",
		"pkg": "4.4.9",
		"ts-jest": "^26.4.1",
		"ts-node": "^9.0.0",
		"typescript": "^4.0.3",
		"uuid": "^8.3.2"
	},
	"scripts": {
		"lint": "eslint --ext ts src",
		"format": "eslint --ext ts src --fix",
		"clean": "rm -rf lib && rm -f tsconfig.tsbuildinfo",
		"compile": "tsc -b",
		"readme": "oclif-dev readme",
		"watch": "tsc -b -w",
		"build": "npm run clean && npm run compile && npm run readme",
		"test": "jest",
		"postpack": "rm -f oclif.manifest.json",
		"prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
		"version": "oclif-dev readme && git add README.md",
		"package": "pkg package.json --out-path=dist_bin --targets=node12-linux-x64,node12-macos-x64,node12-win-x64",
		"test-coverage": "jest --coverage=true"
	}
}
