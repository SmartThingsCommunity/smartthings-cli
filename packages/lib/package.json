		"eslint-plugin-import": "^2.27.5",
		"eslint-plugin-jest": "^27.2.1",
		"jest": "^29.5.0",
		"jest-environment-jsdom": "^29.5.0",
		"jest-extended": "^3.2.4",
		"jest-html-reporters": "^3.1.4",
		"log4js": "^6.6.0",
		"mock-stdin": "^1.0.0",
