		"express": "^4.18.1",
		"get-port": "^5.1.1",
		"inquirer": "^8.2.4",
		"js-yaml": "^4.1.0",
		"lodash.at": "^4.6.0",
		"open": "^8.4.0",
		"os-locale": "^5.0.0",
		"qs": "^6.11.1",
		"table": "^6.8.0"
